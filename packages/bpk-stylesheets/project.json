{
  "name": "bpk-stylesheets",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/bpk-stylesheets/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node build",
        "cwd": "packages/bpk-stylesheets"
      },
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "TZ=Etc/UTC jest --coverage --testPathPatterns='packages/bpk-stylesheets'",
        "cwd": "{workspaceRoot}"
      },
      "outputs": [
        "{workspaceRoot}/coverage"
      ]
    }
  },
  "tags": [
    "type:package",
    "scope:backpack",
    "special-case"
  ]
}
