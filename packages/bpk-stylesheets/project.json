{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "bpk-stylesheets",
  "projectType": "library",
  "sourceRoot": "packages/bpk-stylesheets/src",
  "tags": [
    "scope:backpack"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/bpk-stylesheets"
      },
      "cache": true,
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
